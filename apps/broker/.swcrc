{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
      "tsx": false,
    },
    "target": "es2021",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": true
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "noInterop": false
  },
  "minify": false
}
